<Window x:Class="SCTUpdater.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SCTUpdater"
        mc:Ignorable="d"
        Title="SCT Updater" Height="450" Width="800">
    <Grid Background="#FF3A3A3A">
        <Button Content="Button" HorizontalAlignment="Left" Margin="60,326,0,0" VerticalAlignment="Top" Height="49" Width="119" Click="Button_Click" />
        <CheckBox x:Name="cidname" Content="cidname" HorizontalAlignment="Left" Margin="60,20,0,0" VerticalAlignment="Top" Foreground="White" />
        <CheckBox Content="password" HorizontalAlignment="Left" Margin="60,40,0,0" VerticalAlignment="Top" Foreground="White"/>
        <CheckBox Content="cdplc" HorizontalAlignment="Left" Margin="60,60,0,0" VerticalAlignment="Top" Foreground="White"/>
        <CheckBox Content="tags" HorizontalAlignment="Left" Margin="60,80,0,0" VerticalAlignment="Top" Foreground="White"/>
        <CheckBox Content="icons" HorizontalAlignment="Left" Margin="60,100,0,0" VerticalAlignment="Top" Foreground="White"/>
        <TextBox HorizontalAlignment="Left" Margin="570,60,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="120" x:Name="name"/>
        <TextBox HorizontalAlignment="Left" Margin="570,80,0,0" TextWrapping="Wrap" Text="password" VerticalAlignment="Top" Width="120" x:Name="password"/>
        <TextBox HorizontalAlignment="Left" Margin="570,100,0,0" TextWrapping="Wrap" Text="cpdlc" VerticalAlignment="Top" Width="120" x:Name="cpdlc"/>
        <TextBox HorizontalAlignment="Left" Margin="570,120,0,0" TextWrapping="Wrap" Text="extras" VerticalAlignment="Top" Width="120" x:Name="extras"/>
        <Button x:Name="savecreds" Content="save" HorizontalAlignment="Left" Margin="570,152,0,0" VerticalAlignment="Top" Click="savecreds_Click"/>



    </Grid>
</Window>
